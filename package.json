{
  "name": "event-pump-cli",
  "private": true,
  "version": "1.0.0",
  "description": "A tool to seed data-sources with events",
  "author": "Lee Nattress",
  "keywords": [
    "eventstore",
    "eventsource",
    "eventdriven",
    "events",
    "hydrate"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "ts-node ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "npm run clean && tsc",
    "test": "npm run build && ava --verbose",
    "bundle": "npm run build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "ava": "^5.1.0",
    "pkg": "^5.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1266.0",
    "commander": "^9.4.1",
    "js-yaml": "^4.1.0",
    "mssql": "^9.1.1",
    "yup": "^0.32.11"
  }
}
