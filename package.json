{
    "name": "@3t-transform/event-pump",
    "private": false,
    "version": "1.0.19",
    "description": "A tool to seed data-sources with events",
    "author": "3T Transform",
    "keywords": [
        "eventstore",
        "eventsource",
        "eventdriven",
        "events",
        "hydrate"
    ],
    "license": "MIT",
    "main": "./dist/index.js",
    "bin": "./dist/cli.js",
    "files": [
        "dist/**/*",
        "!dist/**/*.test.js"
    ],
    "ava": {
        "files": [
            "src/tests/unit/**/*.test.ts"
        ],
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "failWithoutAssertions": false
    },
    "scripts": {
        "dev": "ts-node ./src/cli.ts",
        "clean": "rimraf ./dist/ ./exec/",
        "build": "npm run clean && tsc",
        "test": "npm run build && ava",
        "bundle": "npm run build && pkg . --out-dir ./exec/",
        "lint": "eslint src/**/*.{js,ts}",
        "lint:generate": "eslint src/**/*.{js,ts} -o output.eslint.json -f json",
        "lint:fix": "eslint src/**/*.{js,ts} --fix"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "ajv": "^8.12.0",
        "ava": "^5.1.0",
        "eslint": "^8.39.0",
        "pkg": "^5.8.0",
        "proxyquire": "^2.1.3",
        "rimraf": "^3.0.2",
        "sinon": "^17.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "@aws-sdk/client-eventbridge": "^3.395.0",
        "@aws-sdk/client-lambda": "^3.414.0",
        "@fast-csv/parse": "^4.3.6",
        "@opensearch-project/opensearch": "^2.0.0",
        "ansi-colors": "^4.1.3",
        "aws-sdk": "^2.1266.0",
        "cli-progress": "^3.12.0",
        "commander": "^9.4.1",
        "csv-parse": "^5.3.5",
        "dotenv": "^16.0.1",
        "handlebars": "^4.7.7",
        "id128": "^1.6.6",
        "ion-js": "^5.2.0",
        "js-yaml": "^4.1.0",
        "moment": "^2.29.4",
        "mssql": "^9.1.1",
        "nanoid": "^4.0.1",
        "pg": "^8.11.0",
        "sqlite3": "^5.1.4",
        "uuid": "^9.0.1",
        "yup": "^0.32.11"
    }
}
